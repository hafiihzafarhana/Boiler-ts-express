    "pg-hstore": "^2.3.4",
    "pino-pretty": "^11.3.0",
    "sequelize": "^6.37.5",
    "tsconfig-paths": "^4.2.0",
    "uuid": "^11.0.2",
    "winston": "^3.15.0"
